{
  "expo": {
    "name": "PharmacyDashboard",
    "slug": "pharmacy-dashboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDF6EC"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.pharmacydashboard" 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FDF6EC"
      },
      "package": "com.yourcompany.pharmacydashboard"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [],
    "jsEngine": "hermes",
    "sdkVersion": "50.0.0" 
  }
} 